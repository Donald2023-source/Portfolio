{
      "version": 2,
      "builds": [
        {
          "src": "src/api/index.js",
          "use": "@vercel/node"
        },
        {
          "src": "dist/**/*",
          "use": "@vercel/static"
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/src/api/index.js"
        },
        {
          "src": "/assets/(.*)",
          "headers": {
            "cache-control": "public, max-age=31536000, immutable"
          },
          "dest": "dist/assets/$1"
        },
        {
          "src": "/(.*)",
          "dest": "dist/index.html"
        }
      ],
      "env": {
        "NODE_ENV": "production",
        "VITE_API_URL": "@my-api-url"
      }
    }