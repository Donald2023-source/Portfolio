{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
    "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/_next/image(.*)",
      "dest": "/_next/image$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "x-vercel-rewrite": "true"
      }
    }
  ]
}