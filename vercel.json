{
       "version": 2,
       "builds": [
         {
           "src": "dist/**/*",
           "use": "@vercel/static"
         }
       ],
       "routes": [
         {
           "src": "/assets/(.*)",
           "headers": {
             "Cache-Control": "public, max-age=31536000, immutable"
           },
           "dest": "dist/assets/$1"
         },
         {
           "src": "/(.*)",
           "dest": "dist/index.html"
         }
       ],
       "env": {
         "NODE_ENV": "production"
       }
     }